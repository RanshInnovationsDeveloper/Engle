{
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ]
      
    }
  ],
  "hosting": {
    "public": "public",
    "rewrites": [
      { "source": "/app/**", "function": "app" }
    ]
  }
}
